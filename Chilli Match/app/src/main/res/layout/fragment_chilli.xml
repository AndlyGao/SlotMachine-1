<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ChilliFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/hor1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.09" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/hor2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.79" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/vert1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.149" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/vert2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.85" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/chillislots"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="@+id/hor1"
            app:layout_constraintBottom_toBottomOf="@+id/hor2"
            app:layout_constraintStart_toStartOf="@+id/vert1"
            app:layout_constraintEnd_toEndOf="@+id/vert2"
            android:background="@drawable/back_inside">


            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/vf_hor1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.33" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/vf_hor2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.66" />



            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/vf_vert1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.2" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/vf_vert2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.4" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/vf_vert3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.6" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/vf_vert4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.8" />

            <ViewFlipper
                android:id="@+id/image1"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/vf_vert1"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="@+id/vf_hor1" />

            <ViewFlipper
                android:id="@+id/image2"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/vf_vert1"
                app:layout_constraintTop_toTopOf="@+id/vf_hor1"
                app:layout_constraintBottom_toBottomOf="@+id/vf_hor2" />

            <ViewFlipper
                android:id="@+id/image3"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/vf_vert1"
                app:layout_constraintTop_toTopOf="@+id/vf_hor2"
                app:layout_constraintBottom_toBottomOf="parent" />



            <ViewFlipper
                android:id="@+id/image4"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintStart_toStartOf="@+id/vf_vert1"
                app:layout_constraintEnd_toEndOf="@+id/vf_vert2"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="@+id/vf_hor1" />

            <ViewFlipper
                android:id="@+id/image5"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintStart_toStartOf="@+id/vf_vert1"
                app:layout_constraintEnd_toEndOf="@+id/vf_vert2"
                app:layout_constraintTop_toTopOf="@+id/vf_hor1"
                app:layout_constraintBottom_toBottomOf="@+id/vf_hor2" />

            <ViewFlipper
                android:id="@+id/image6"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintStart_toStartOf="@+id/vf_vert1"
                app:layout_constraintEnd_toEndOf="@+id/vf_vert2"
                app:layout_constraintTop_toTopOf="@+id/vf_hor2"
                app:layout_constraintBottom_toBottomOf="parent" />




            <ViewFlipper
                android:id="@+id/image7"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintStart_toStartOf="@+id/vf_vert2"
                app:layout_constraintEnd_toEndOf="@+id/vf_vert3"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="@+id/vf_hor1" />

            <ViewFlipper
                android:id="@+id/image8"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintStart_toStartOf="@+id/vf_vert2"
                app:layout_constraintEnd_toEndOf="@+id/vf_vert3"
                app:layout_constraintTop_toTopOf="@+id/vf_hor1"
                app:layout_constraintBottom_toBottomOf="@+id/vf_hor2" />

            <ViewFlipper
                android:id="@+id/image9"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintStart_toStartOf="@+id/vf_vert2"
                app:layout_constraintEnd_toEndOf="@+id/vf_vert3"
                app:layout_constraintTop_toTopOf="@+id/vf_hor2"
                app:layout_constraintBottom_toBottomOf="parent" />




            <ViewFlipper
                android:id="@+id/image10"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintStart_toStartOf="@+id/vf_vert3"
                app:layout_constraintEnd_toEndOf="@+id/vf_vert4"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="@+id/vf_hor1" />

            <ViewFlipper
                android:id="@+id/image11"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintStart_toStartOf="@+id/vf_vert3"
                app:layout_constraintEnd_toEndOf="@+id/vf_vert4"
                app:layout_constraintTop_toTopOf="@+id/vf_hor1"
                app:layout_constraintBottom_toBottomOf="@+id/vf_hor2" />

            <ViewFlipper
                android:id="@+id/image12"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintStart_toStartOf="@+id/vf_vert3"
                app:layout_constraintEnd_toEndOf="@+id/vf_vert4"
                app:layout_constraintTop_toTopOf="@+id/vf_hor2"
                app:layout_constraintBottom_toBottomOf="parent" />



            <ViewFlipper
                android:id="@+id/image13"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintStart_toStartOf="@+id/vf_vert4"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="@+id/vf_hor1" />

            <ViewFlipper
                android:id="@+id/image14"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintStart_toStartOf="@+id/vf_vert4"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@+id/vf_hor1"
                app:layout_constraintBottom_toBottomOf="@+id/vf_hor2" />

            <ViewFlipper
                android:id="@+id/image15"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintStart_toStartOf="@+id/vf_vert4"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@+id/vf_hor2"
                app:layout_constraintBottom_toBottomOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>


            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/back_outside"/>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/hor3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.79" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/hor4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.85" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/hor5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.88" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/hor6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.93" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/vert3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.13" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/vert4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.16" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/vert5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.25" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/vert6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.28" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/vert7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.39" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/vert8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.61" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/vert9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.66" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/vert10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.715" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/vert11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.74" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/vert12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.833" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/vert13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.05" />

        <ImageView
            android:id="@+id/info_button"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/vert13"
            app:layout_constraintTop_toTopOf="@+id/hor6"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="5dp"
            android:src="@drawable/info_button"
            />


        <ImageView
            android:id="@+id/minus_button"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintStart_toStartOf="@+id/vert3"
            app:layout_constraintEnd_toEndOf="@+id/vert4"
            app:layout_constraintTop_toTopOf="@+id/hor3"
            app:layout_constraintBottom_toBottomOf="@+id/hor4"
            android:src="@drawable/minus_button"
            />

        <TextView
            android:id="@+id/bet_txt"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="@+id/hor3"
            app:layout_constraintBottom_toBottomOf="@+id/hor4"
            app:layout_constraintStart_toStartOf="@+id/vert4"
            app:layout_constraintEnd_toEndOf="@+id/vert5"
            android:textStyle="bold"
            android:textSize="12dp"
            android:text="0.5"
            android:gravity="center_horizontal|center_vertical"
            android:fontFamily="sans-serif-black"
            android:textColor="#ffffff"/>


        <ImageView
            android:id="@+id/plus_button"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintStart_toStartOf="@+id/vert5"
            app:layout_constraintEnd_toEndOf="@+id/vert6"
            app:layout_constraintTop_toTopOf="@+id/hor3"
            app:layout_constraintBottom_toBottomOf="@+id/hor4"
            android:src="@drawable/plus_button"
            />



        <ImageView
            android:id="@+id/stop_auto_game_button"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintStart_toStartOf="@+id/vert6"
            app:layout_constraintEnd_toEndOf="@+id/vert7"
            app:layout_constraintTop_toTopOf="@+id/hor4"
            app:layout_constraintBottom_toBottomOf="parent"
            android:src="@drawable/stop_auto_game_button"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="5dp"
            />

        <ImageView
            android:id="@+id/auto_game_button"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintStart_toStartOf="@+id/vert6"
            app:layout_constraintEnd_toEndOf="@+id/vert7"
            app:layout_constraintTop_toTopOf="@+id/hor4"
            app:layout_constraintBottom_toBottomOf="parent"
            android:src="@drawable/auto_game_button"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="5dp"
            />

        <ImageView
            android:id="@+id/spin_button"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintStart_toStartOf="@+id/vert8"
            app:layout_constraintEnd_toEndOf="@+id/vert10"
            app:layout_constraintTop_toTopOf="@+id/hor4"
            app:layout_constraintBottom_toBottomOf="parent"
            android:src="@drawable/spin_button"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="5dp"
            />



        <TextView
            android:id="@+id/x_txt"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="@+id/hor3"
            app:layout_constraintBottom_toBottomOf="@+id/hor4"
            app:layout_constraintStart_toStartOf="@+id/vert8"
            app:layout_constraintEnd_toStartOf="@+id/aditional_multiplier_txt"
            android:textStyle="bold"
            android:textSize="12dp"
            android:text="x "
            android:gravity="center_vertical|center_horizontal"
            android:textColor="#ffffff"/>

        <TextView
            android:id="@+id/aditional_multiplier_txt"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="@+id/hor3"
            app:layout_constraintBottom_toBottomOf="@+id/hor4"
            app:layout_constraintStart_toEndOf="@+id/x_txt"
            app:layout_constraintEnd_toEndOf="@+id/vert9"
            android:textStyle="bold"
            android:textSize="12dp"
            android:text="1"
            android:gravity="center_vertical"
            android:textColor="#ffffff"/>

        <TextView
            android:id="@+id/balance_txt"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="@+id/hor3"
            app:layout_constraintBottom_toBottomOf="@+id/hor4"
            app:layout_constraintStart_toStartOf="@+id/vert11"
            app:layout_constraintEnd_toEndOf="@+id/vert12"
            android:textStyle="bold"
            android:textSize="12dp"
            android:text="1000.0"
            android:gravity="center_horizontal|center_vertical"
            android:fontFamily="sans-serif-black"
            android:textColor="#ffffff"/>

        <TextView
            android:id="@+id/prize_txt"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="@+id/hor5"
            app:layout_constraintBottom_toBottomOf="@+id/hor6"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:textStyle="bold"
            android:textSize="12dp"
            android:text="0.0"
            android:gravity="center_horizontal|center_vertical"
            android:fontFamily="sans-serif-black"
            android:textColor="#ffffff"/>


        <ImageView
            android:id="@+id/bonus_button"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintStart_toStartOf="@+id/vert12"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/hor5"
            app:layout_constraintBottom_toBottomOf="parent"
            android:src="@drawable/bonus_button"
            android:layout_marginTop="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp"
            />

        <ImageView
            android:id="@+id/you_won"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintStart_toStartOf="@+id/vert6"
            app:layout_constraintEnd_toEndOf="@+id/vert10"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:src="@drawable/you_won"
            android:visibility="gone"
            />

        <ImageView
            android:id="@+id/hugewin_caption"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintStart_toStartOf="@+id/vert6"
            app:layout_constraintEnd_toEndOf="@+id/vert10"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:src="@drawable/hugewin_caption"
            android:visibility="gone"
            />

        <ImageView
            android:id="@+id/megawin_caption"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintStart_toStartOf="@+id/vert6"
            app:layout_constraintEnd_toEndOf="@+id/vert10"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:src="@drawable/megawin_caption"
            android:visibility="gone"
            />

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>